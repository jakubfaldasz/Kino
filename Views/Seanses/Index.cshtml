@model IEnumerable<KinoDotNetCore.Models.Seanse>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Dzien)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Godzina)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Filmy.Tytul)
            </th>
            <th>
               @Html.DisplayNameFor(model => model.Sale.Id)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Dzien)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Godzina)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Filmy.Tytul)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Sale.Id)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
